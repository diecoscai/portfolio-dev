---
const { project } = Astro.props;
console.log(project);
---
<div
class="max-w-sm bg-[#252422] dark:bg-[#FFFCF2] border border-gray-200 rounded-lg shadow-md"
>
<a href="#">
  <img
    class="rounded-t-lg"
    src="/projects_img/plant-faced.png"
    alt=""
  />
</a>
<div class="p-5">
  <a href="#">
    <h5
      class="mb-2 text-2xl font-bold tracking-tight text-[#FFFCF2] dark:text-[#403D39]"
    >
      {project.name}
    </h5>
  </a>
  <p class="mb-3 font-normal text-[#CCC5B9] dark:text-[#403D39]">
    {project.description}
  </p>
  <div class="flex space-x-2 justify-center">
    <a
      href="#"
      class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"
    >
      Read more
      <svg
        class="rtl:rotate-180 w-3.5 h-3.5 ms-2"
        aria-hidden="true"
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 14 10"
      >
        <path
          stroke="currentColor"
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M1 5h12m0 0L9 1m4 4L9 9"></path>
      </svg>
    </a>
    <a
      href="#"
      class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-[#252422] dark:text-[#FFFCF2] bg-[#FFFCF2] dark:bg-[#403D39] rounded-lg hover:bg-[#403D39] dark:hover:bg-[#252422] focus:ring-4 focus:outline-none focus:ring-blue-300   -700 dark:focus:ring-[#252422]"
    >
      Read more
      <svg
        class="rtl:rotate-180 w-3.5 h-3.5 ms-2"
        aria-hidden="true"
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 14 10"
      >
        <path
          stroke="currentColor"
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M1 5h12m0 0L9 1m4 4L9 9"></path>
      </svg>  
    </a>
  </div>
</div>
</div>